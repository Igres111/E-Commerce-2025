    <ImplicitUsings>enable</ImplicitUsings>
    <RootNamespace>E_Commerce_2025</RootNamespace>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="DotNetEnv" Version="3.1.1" />
